{
  "name": "next-fortress",
  "version": "0.0.0-development",
  "description": "This is a Next.js plugin that blocks, redirects, or displays a dummy page for accesses that are not authenticated.",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "repository": "git@github.com:aiji42/next-fortress.git",
  "author": "aiji42 <aiji42@gmail.com> (https://twitter.com/aiji42_dev)",
  "license": "MIT",
  "keywords": [
    "next.js",
    "next",
    "react",
    "plugins",
    "access controll",
    "content block"
  ],
  "bugs": {
    "url": "https://github.com/aiji42/next-fortress/issues"
  },
  "homepage": "https://github.com/aiji42/next-fortress#readme",
  "scripts": {
    "lint": "eslint ./src/**/*.ts ./src/*.ts",
    "format": "prettier --write src/*.ts src/**/*.ts example/src/*",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc && ts-add-module-exports",
    "prepare": "husky install",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@auth0/nextjs-auth0": ">=1.0.0",
    "aws-amplify": ">=3.0.0",
    "firebase": ">=7.8.0",
    "next": ">=10.2.0",
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@auth0/nextjs-auth0": "^1.5.0",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-typescript": "^7.14.5",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "aws-amplify": "^4.2.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "firebase": "^8.8.1",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "next": "^11.0.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "semantic-release": "^17.4.4",
    "semantic-release-cli": "^5.4.3",
    "ts-add-module-exports": "^1.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "firebase-admin": "^9.11.0",
    "ip-cidr": "^3.0.3",
    "nookies": "^2.5.2"
  }
